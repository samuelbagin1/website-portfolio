{
    "version": 2,
    "builds": [
      {
        "src": "src/api/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/index.js",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/src/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/src/index.js"
      }
    ]
  }